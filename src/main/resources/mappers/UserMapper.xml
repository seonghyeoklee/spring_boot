<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0/EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.boot1.dao.UserDAO">
    <insert id="insertUser" keyProperty="idx" useGeneratedKeys="true">
        insert into user(
          name
        ) values (
          #{name}
        )
    </insert>

</mapper>